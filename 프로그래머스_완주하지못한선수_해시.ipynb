    "from collections import Counter\n",
    "def solution(participant, completion):\n",
    "    p_c = Counter(participant)\n",
    "    c_c = Counter(completion)    \n",
    "    answer = ''.join(list(p_c - c_c))\n",
    "    return answer"
   ]
  }
